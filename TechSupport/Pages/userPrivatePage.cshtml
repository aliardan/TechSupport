<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Technical Support</title>
    <link rel="stylesheet" href="~/css/userPrivatePage.css">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&display=swap" rel="stylesheet">
</head>
<body>
    <header class="header">
        <div class="logo">

        </div>
        <div class="menu">

        </div>
    </header>
    <div class="content">
        <div class="list">
            <table cellspacing="0" , width="500px" , bgcolor=white>
                <tr bgcolor="#1E2734" style="margin-left: 10px; height: 75px;">
                    <td width="40%">
                        <p style="color: white; margin-left: 10px;">Список задач</p>
                    </td>
                    <td>
                        <button class="btn-buttonAddTask" type="button" style="height: 45px;">Добавить задачу</button>
                    </td>
                </tr>
                @foreach (var item in Model)
                {
                    <tr style="margin-left: 10px; height: 75px;">
                        <td colspan="3">
                            <button class="btn-buttonOpenTask1" type="button"> @item.Name</button>

                            В работе
                            Приоритет неотложенный
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</body>
</html>
